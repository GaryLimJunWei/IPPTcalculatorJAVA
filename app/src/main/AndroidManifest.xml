<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.garylim.ipptcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Main Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewTable"
            android:label="New Table" />
        <activity android:name=".Settings" />
        <activity
            android:name=".scorePage"
            android:label="@string/title_activity_score_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Dbhelper" />
        <activity
            android:name=".tableList"
            android:label="@string/title_activity_table_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".summarise" />
    </application>

</manifest>